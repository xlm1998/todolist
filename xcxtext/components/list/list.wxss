/* components/list/list.wxss */
.list-x{
  width: 100%;
 height: 120px;
 border: 2px solid #ccc;
 display: flex;
 align-items: center;
 
 }
 .list-x .list-l{
   width: 140px;
   height: 100px;
 }
 .list-x .list-l image{
   width:100%;
   height: 100%;
 }
 .list-x .list-r{
 padding-left: 10px;
 box-sizing: border-box;
 flex: 1;
 height: 100px;
 display: flex;
 flex-direction: column;
 justify-content: space-around;
 }
 .list-x .list-r :nth-child(1){
   font-weight: 700;
   font-size: 20px;
 }
 .list-x .list-r :nth-child(2){
 
   font-size: 14px;
 }
 .list-x .list-r button{
   display: block;
   margin-left: 0px;
   font-size: 14px;
   width: 100px;
   height: 30px;
   background-color: orange;
   line-height: 30px;
 }
 