/* pages/log/log.wxss */
.todolist{
  width: 100%;
  height: 100%;
  background: #eee;
}
.header{
  width: 100%;
  height: 50px;
  display: flex;
  align-items: center;
  justify-content: space-around;
  background: #000;
  color: #ccc;

}
.shu{
  width: 100%;
  height: 40px;
  line-height: 40px;
 padding-left: 20px;
 box-sizing: border-box;
}

.header input{
  width: 60%;
  height: 40px;
  background: #fff;
  border-radius: 5px;
  font-size: 14px;
  padding-left: 10px;
  box-sizing: border-box;
}
.list{
  width: 100%;
  height: 40px;
  display: flex;
  align-items: center;
 justify-content: space-around;
  padding: 0 10px;
  box-sizing: border-box;
  background: #fff;
  margin-bottom: 10px;
}
.inp{
  border: 1px solid #ccc;
  padding-left: 10px;
  box-sizing: border-box;
}